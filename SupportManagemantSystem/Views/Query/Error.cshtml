
@{
    ViewBag.Title = "ارسال خطا";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@ViewBag.message</h2>
<form action="/query/error" enctype="multipart/form-data" method="post">
    <div class="form-group" style="direction:rtl">
        <label>ارسال فایل</label>
        <input type="file" name="file" value="" accept=".json" />
    </div>
    <div class="form-group" style="direction:rtl">
        <input type="submit" class="btn btn-default" name="send" value="ارسال" />
    </div>
</form>

